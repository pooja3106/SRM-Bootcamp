public class Main
{
    public static int fibRecursive(int n ){
        if( n <= 1)return n;
        return fibRecursive(n - 1) + fibRecursive(n - 2);
    }
    
	public static void main(String[] args) {
	    int n = 10;
	    for (int i = 0; i < n ; i ++){
	        System.out.println(fibRecursive(i) + "");
	    }
	}
}
